<%- include('../partials/header') %>
    <div class="container ">
        <div class="main-body">
            <div class="row gutters-sm">
                <div class="card mb-3">
                    <div class="card-body">
                        <h5 class="text-danger border-bottom border-danger my-3">
                            <%= Project.Title %>
                        </h5>
                        <div class="row">
                            <div class="col-sm-3">
                                <h6 class="mb-0">Employee</h6>
                            </div>
                            <div class="col-sm-9 text-secondary">
                                <%= Project.employeeId %>
                            </div>
                        </div>
                        <hr>
                        <div class="row">
                            <div class="col-sm-3">
                                <h6 class="mb-0">Submission Date</h6>
                            </div>
                            <div class="col-sm-9 text-secondary">
                                <%= moment(Project.Createdin).format('LL') %>
                            </div>
                        </div>
                        <hr>
                        <div class="row">
                            <div class="col-sm-3">
                                <h6 class="mb-0">Starts</h6>
                            </div>
                            <div class="col-sm-9 text-secondary">
                                <%= moment(Project.startDate).format('LL') %>
                            </div>
                        </div>
                        <hr>
                        <div class="row">
                            <div class="col-sm-3">
                                <h6 class="mb-0">Due date</h6>
                            </div>
                            <div class="col-sm-9 text-secondary">
                                <%= moment(Project.dueDate).format('LL') %>
                            </div>
                        </div>
                        <hr>
                        <div class="row">
                            <div class="col-sm-3">
                                <h6 class="mb-0">Status</h6>
                            </div>
                            <div class="col-sm-9 text-secondary">
                                <%= Project.ProjectStatus %>
                            </div>
                        </div>
                        <hr>
                        <div class="row">
                            <div class="col-sm-3">
                                <h6 class="mb-0">Created By </h6>
                            </div>
                            <div class="col-sm-9 text-secondary">
                                <%= Project.createdBy %>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
        </div>
    </div>