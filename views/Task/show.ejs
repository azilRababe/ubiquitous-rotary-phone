<%- include('../partials/header') %>
    <br>
    <div class="container">
        <%- include('../partials/messages') %>
            <h4 class="text-danger">

                <% Task.assignedTo.forEach(e=>{ %>
                    THIS TASK HAS BEEN ASSIGNED TO
                    <b>
                        <%= `${e.Firstname} ${e.Lastname}` %>
                    </b>
                    <% }) %>
                        <% Task.assignerId.forEach(e=>{ %>
                            BY <b>
                                <%= `${e.Firstname} ${e.Lastname}` %>
                            </b>
                            <% }) %>
            </h4> <br>
            <b>Assign Date:</b>
            <%= moment(Task.assignDate).format('L') %> <br>

                <b>startDate:</b>
                <%= moment(Task.startDate).format('L') %> <br>

                    <b>dueDate:</b>
                    <%= moment(Task.dueDate).format('L')%> <br>

                        <b>Notes:</b>
                        <%= Task.Notes %> <br>

                            <b>task Status :</b>
                            <%= Task.taskStatus %> <br>





    </div>